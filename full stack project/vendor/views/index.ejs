<!DOCTYPE html>
<html>
  <head>
    <title>Product List</title>
  </head>
  <body>
    <h1>Product List</h1>
    <ul>
      <% products.forEach(function(product) { %>
        <li>
          <a href="/products/<%= product._id %>"><%= product.Product_name %></a>
        </li>
      <% }); %>
    </ul>
    <a href="/add">Add Product</a>
    <a href="/viewmyproduct">View My Products</a>
  </body>
</html>
